    "babel-eslint": "^10.0.1",
    "cross-env": "^5.0.1",
    "del": "^3.0.0",
    "eslint": "^4.13.1",
    "eslint-config-brightspace": "^0.4.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-html": "~4.0.1",
    "frau-ci": "^1.33.2",
    "gulp": "^4.0.0",
