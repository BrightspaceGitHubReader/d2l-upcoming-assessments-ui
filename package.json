  "devDependencies": {
    "bower": "^1.8.0",
    "eslint": "^3.17.0",
    "eslint-config-brightspace": "^0.2.1",
    "eslint-config-google": "^0.7.0",
    "eslint-plugin-html": "^1.7.0",
    "frau-ci": "^1.33.2",
    "polymer-cli": "^0.17.0"
  }
